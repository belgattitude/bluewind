{
    "name": "@bluewind/frontend",
    "version": "0.0.1",
    "description": "Classroom application frontend",
    "author": "Vanvelthem SÃ©bastien",
    "license": "AGPL-3.0",
    "scripts": {
        "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --mode production --config config/webpack.production.js",
        "source-map-explorer": "source-map-explorer ./dist/main.*.js ./dist/main.*.js.map",
        "stats": "yarn build && webpack-bundle-analyzer dist/.webpack-stats.json",
        "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config config/webpack.development.js",
        "clean:all": "run-p clean:build clean:dist",
        "clean:build": "rimraf build",
        "clean:dist": "rimraf dist",
        "tsc": "tsc -p . --noEmit",
        "lint": "tslint --project './tsconfig.json'",
        "lint:fix": "tslint --project './tsconfig.json' --fix",
        "format": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json}\" \"./server/**/*.{ts,tsx,scss,js,json}\" --write \"./*.{js,json}\"",
        "test": "jest --config jest.config.js",
        "test:coverage": "jest --config jest.config.js --coverage",
        "test:watch": "jest --config jest.config.js --watch",
        "test:update": "jest --config jest.config.js -u"
    },
    "husky": {
        "hooks": {
            "post-commit": "git diff --name-only HEAD^ | xargs git add",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
