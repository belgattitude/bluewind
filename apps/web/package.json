  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --mode production --config webpack.production.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config webpack.development.js",
    "clean": "rimraf --no-glob  build dist",
    "typecheck": "tsc -p . --noEmit",
    "es-check": "es-check es5 './dist/**/*.js' -v ",
    "format": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "analyze": "webpack-bundle-analyzer dist/.webpack-stats.json dist/public",
    "precommit": "lint-staged",
    "coverage:publish": "codecov --root=../../ -F apps",
    "deps:check": "npx npm-check-updates --dep prod,dev,optional",
    "deps:update": "npx npm-check-updates --dep prod,dev,optional -u"
  },
  "lint-staged": {
    "*.{json,css,scss}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --rule 'react-hooks/exhaustive-deps: off'"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
