{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "allowJs":true,
        "checkJs": false,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "importHelpers": false,
        "noEmitHelpers": false,
        "isolatedModules": false
    }
}
